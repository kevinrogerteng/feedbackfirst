<form role="form" ng-submit="attemptLogin()">
  <h1> Sign in! </h1>
  <div class="form-group">
    <label>Email address</label>
    <input type="email" class="form-control" placeholder="Enter email" ng-model="credentials.email">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input type="password" class="form-control" ng-model="credentials.password" placeholder="Password">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
